pipeline {

    agent any

    tools {

        maven ‘mvn’

    }

    stages {
			stage 'Access repository'
			node {

			git branch: 'master', 
			url: 'https://github.com/ColmCharlton/javaWebWordRetriever'
				
	}
        stage('Example') {

            steps {

                sh 'mvn --version'

            }

        }

    }

}